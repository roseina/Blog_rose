
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  
  <!-- Left side column. contains the logo and sidebar -->
 

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    

    <!-- Main content -->
    <section class="content">
    <%= render 'admin/articles/partials/edit' %>
  
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
 
  <!-- /.control-sidebar -->
  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->

<!-- ./wrapper -->

<!-- jQuery 2.2.3 -->
<%= javascript_include_tag "plugins/jQuery/jquery-2.2.3.min.js" %> 
<!-- jQuery UI 1.11.4 -->
<%= javascript_include_tag "https://code.jquery.com/ui/1.11.4/jquery-ui.min.js" %> 
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
  $.widget.bridge('uibutton', $.ui.button);
  </script>
 
<!-- Bootstrap 3.3.6 -->
<%= javascript_include_tag "js/bootstrap.min.js" %> 
<!-- Morris.js charts -->
<%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js" %> 
<%= javascript_include_tag "plugins/morris/morris.min.js" %> 
<!-- Sparkline -->
<%= javascript_include_tag "plugins/sparkline/jquery.sparkline.min.js" %> 
<!-- jvectormap -->
<%= javascript_include_tag "plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" %> 
<%= javascript_include_tag "plugins/jvectormap/jquery-jvectormap-world-mill-en.js" %> 
<!-- jQuery Knob Chart -->
<%= javascript_include_tag "plugins/knob/jquery.knob.js" %> 
<!-- daterangepicker -->
<%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js" %> 
<%= javascript_include_tag "plugins/daterangepicker/daterangepicker.js" %> 
<!-- datepicker -->
<%= javascript_include_tag "plugins/datepicker/bootstrap-datepicker.js" %> 
<!-- Bootstrap WYSIHTML5 -->
<%= javascript_include_tag "plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js" %> 
<!-- Slimscroll -->
<%= javascript_include_tag "plugins/slimScroll/jquery.slimscroll.min.js" %> 
<!-- FastClick -->
<%= javascript_include_tag "plugins/fastclick/fastclick.js" %> 
<!-- AdminLTE App -->
<%= javascript_include_tag "js/app.min.js" %> 
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<%= javascript_include_tag "js/pages/dashboard.js" %> 
<%= javascript_include_tag "js/demo.js" %> 

